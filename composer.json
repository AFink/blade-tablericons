{
    "name": "afink/blade-tablericons",
    "description": "A package to easily make use of tabler-icons in your Laravel Blade views.",
    "keywords": [
        "blade-tablericons"
    ],
    "homepage": "https://github.com/AFink/blade-tablericons",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Andreas Fink",
            "email": "andreas@andreasfink.xyz",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2|^7.3|^7.4|^8.0",
        "illuminate/support": "^7.14.0|^8.0",
        "blade-ui-kit/blade-icons": "^0.4|^0.5"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "AFink\\BladeTablericons\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AFink\\BladeTablericons\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "AFink\\BladeTablericons\\BladeTablericonsServiceProvider"
            ]
        }
    }
}
